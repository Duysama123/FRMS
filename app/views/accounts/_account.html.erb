<div id="<%= dom_id account %>">
  <p>
    <strong>Username:</strong>
    <%= account.username %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= account.email %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= account.password %>
  </p>

  <p>
    <strong>Role:</strong>
    <%= account.role %>
  </p>
<p>
    <strong>Image:</strong>
    <%= account.image %>
  </p>
</div>
